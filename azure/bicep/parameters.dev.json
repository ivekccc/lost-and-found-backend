{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "lost-and-found"
    },
    "environment": {
      "value": "dev"
    },
    "postgresAdminLogin": {
      "value": "pgadmin"
    },
    "postgresAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/REPLACE_WITH_SUBSCRIPTION_ID/resourceGroups/REPLACE_WITH_RG_NAME/providers/Microsoft.KeyVault/vaults/REPLACE_WITH_VAULT_NAME"
        },
        "secretName": "postgres-admin-password"
      }
    },
    "jwtSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/REPLACE_WITH_SUBSCRIPTION_ID/resourceGroups/REPLACE_WITH_RG_NAME/providers/Microsoft.KeyVault/vaults/REPLACE_WITH_VAULT_NAME"
        },
        "secretName": "jwt-secret"
      }
    },
    "mailUsername": {
      "value": "REPLACE_WITH_YOUR_EMAIL@example.com"
    },
    "mailPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/REPLACE_WITH_SUBSCRIPTION_ID/resourceGroups/REPLACE_WITH_RG_NAME/providers/Microsoft.KeyVault/vaults/REPLACE_WITH_VAULT_NAME"
        },
        "secretName": "mail-password"
      }
    },
    "appServicePlanSku": {
      "value": "B1"
    }
  }
}
